{
  "name": "cartesi-battleships",
  "version": "1.0.0",
  "description": "CLI FrontEnd client for Cartesi Battleships game",
  "main": "./bin/index.js",
  "scripts": {
    "codegen": "run-p codegen:typechain codegen:graphql",
    "codegen:graphql": "graphql-codegen",
    "codegen:typechain": "typechain --target ethers-v5 --out-dir generated-src/rollups ../contracts/node_modules/@cartesi/rollups/export/artifacts/*.json",
    "build": "run-s codegen tsc",
    "build2": "run-s codegen:typechain tsc",
    "start": "ts-node ./dist/bin/index.js",
    "start-p1": "ts-node ./dist/bin/index.js player1",
    "start-p2": "ts-node ./dist/bin/index.js player2",
    "tsc": "tsc"
  },
  "bin": {
    "cbs": "./bin/index.js"
  },
  "author": "Cartesi",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typed-document-node": "^2.2.5",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@typechain/ethers-v5": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "codegen": "^0.1.0",
    "cross-fetch": "^3.1.5",
    "ethercore-json-rpc-provider": "^4.0.0",
    "ethers": "^5.6.5",
    "graphql": "^16.5.0",
    "graphql-codegen": "^0.4.0",
    "inquirer": "^8.2.4",
    "jetty": "^0.2.1",
    "keypress": "^0.2.1",
    "node-fetch": "^2.6.7",
    "simple-terminal-menu": "^2.0.0"
  }
}
